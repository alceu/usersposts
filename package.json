{
  "name": "usersposts-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.7.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "bootstrap": "^5.1.3",
    "react": "^17.0.2",
    "react-bootstrap": "^2.1.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-scripts": "5.0.0",
    "sass": "^1.49.8"
  },
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "build": "react-scripts build",
    "prepare": "./node_modules/.bin/husky install",
    "lint": "./node_modules/.bin/eslint",
    "lint-all": "npm run lint -- 'src/**/*.js'",
    "lint-fix": "npm run lint -- --fix",
    "lint-fix-all": "npm run lint-all -- --fix",
    "format": "./node_modules/.bin/prettier --config ./.prettierrc.js",
    "format-all": "npm run format -- --write './**/*.{js,css,md,json}'",
    "precommit": "./node_modules/.bin/lint-staged && CI=true npm run test",
    "prepush": "CI=true npm run test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1"
  }
}
